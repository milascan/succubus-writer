{
  "name": "template",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build && cp deno.json build/deno.json",
    "deno": "vite build && cd build && deno serve -A index.js",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@deno/vite-plugin": "latest",
    "@iconify-json/material-symbols": "^1.2.6",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.15.3",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@sveltelegos-blue/svelte-legos": "^0.1.3",
    "@unocss/core": "latest",
    "@unocss/extractor-svelte": "latest",
    "@unocss/preset-icons": "latest",
    "@unocss/preset-uno": "latest",
    "@unocss/reset": "latest",
    "@unocss/svelte-scoped": "latest",
    "@unocss/transformer-directives": "latest",
    "@unocss/transformer-variant-group": "latest",
    "svelte": "^5.7.0",
    "svelte-check": "^4.0.0",
    "tslib": "^2.4.1",
    "typescript": "^5.5.0",
    "unocss": "latest",
    "unocss-preset-radix": "latest",
    "vite": "^5.4.4",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vitest": "^2.1.4"
  },
  "type": "module",
  "dependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@std/ulid": "npm:@jsr/std__ulid",
    "mitt": "^3.0.1",
    "msgpackr": "^1.11.2",
    "rollup": "^4.27.4",
    "runed": "latest"
  }
}
